<setup>
  <block title="Define MISP server parameters" endpoint="misp/customendpoint" entity="mispsetup">
    <input field="misp_url">
      <label>MISP url:</label>
      <type>text</type>
    </input>
    <input field="misp_key" endpoint="misp/customendpoint" entity="mispsetup">
      <label>Set the MISP auth key</label>
      <type>text</type>
    </input>
    <input field="misp_verifycert" endpoint="misp/customendpoint" entity="mispsetup">
      <label>Check SSL certificate of MISP server</label>
      <type>bool</type>
    </input>
  </block>
</setup>